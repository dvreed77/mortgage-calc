{
  "name": "mortgage",
  "version": "0.1.0",
  "homepage": "http://dvreed77.github.io/mortgage-calc",
  "private": true,
  "dependencies": {
    "@craco/craco": "^5.6.2",
    "antd": "^3.26.2",
    "craco-antd": "^1.14.1",
    "katex": "^0.11.1",
    "postcss-cli": "^6.1.3",
    "postcss-import": "^12.0.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-katex": "^2.0.2",
    "react-scripts": "3.3.0",
    "tailwindcss": "^1.1.3"
  },
  "scripts": {
    "start": "yarn watch:css & craco start",
    "build": "yarn build:css && craco build",
    "test": "craco test --env=jsdom",
    "eject": "react-scripts eject",
    "build:css": "postcss tailwind.css -o src/index.css",
    "watch:css": "postcss tailwind.css -o src/index.css -w",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "gh-pages": "^2.1.1",
    "autoprefixer": "^9.7.1"
  }
}
